				-->
				<!--
				The 'tags' property ('exhaust') of this location indicates that vehicle exhaust will emit from this point. 
				-->
				<location tags="exhaust" pos="0.5 0.1 -2.1" rot="-180.0 360.0 -180.0"/>
				<!-- 
